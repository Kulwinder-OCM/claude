{
  "functions": {
    "api/app.py": {
      "runtime": "@vercel/python@4.6.0"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/app" }
  ],
  "env": {
    "CLAUDE_API_KEY": "@claude_api_key",
    "GEMINI_API_KEY": "@gemini_api_key",
    "SCREENSHOT_API_KEY": "@screenshot_api_key",
    "OPENAI_API_KEY": "@openai_api_key"
  }
}